{
  "name": "basic-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-styled-components": "1.3.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-app": "3.1.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "check-dependencies": "1.1.0",
    "clf-date": "0.1.0",
    "compression-webpack-plugin": "1.1.3",
    "css-loader": "0.28.7",
    "cypress": "1.4.0",
    "dotenv": "4.0.0",
    "ejs": "2.5.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "enzyme-to-json": "3.2.2",
    "eslint": "4.12.0",
    "eslint-config-react-app": "2.0.1",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-flow": "1.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-react": "7.5.1",
    "express": "4.16.2",
    "express-static-gzip": "0.3.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "0.60.1",
    "fs-extra": "4.0.2",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "jest-enzyme": "4.0.1",
    "morgan": "1.9.0",
    "object-assign": "4.1.1",
    "piping": "1.0.0-rc.4",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.9",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dev-utils": "4.2.1",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "style-loader": "0.19.0",
    "styled-components": "2.2.4",
    "styled-normalize": "2.2.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.5",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "server": "node scripts/server.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:watch": "node scripts/test.js --watch",
    "test:coverage": "node scripts/test.js --coverage",
    "test:e2e": "yarn cypress run",
    "lint": "yarn eslint src",
    "lint:fix": "yarn eslint src --fix",
    "precommit": "yarn lint && yarn flow",
    "prepush": "yarn lint && yarn flow && yarn test:coverage"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "functions": 80,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/(client|server).{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/testSetup.js",
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": true
        }
      ]
    ]
  },
  "engines": {
    "node": "8.9.x",
    "npm": "5.5.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "standard-flow",
      "react-app"
    ],
    "globals": {
      "cy": true,
      "Cypress": true
    }
  },
  "repository": {
    "url": "https://github.com/jungleminds/basic-project",
    "type": "git"
  }
}
